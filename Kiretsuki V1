    Name = "Credits",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})


	-- _G. stuff 
	_G.MeleeStat = true
	_G.DefenseStat = true
	_G.SwordStat = true
	_G.GunStat = true
	_G.FruitStat = true



	-- Functions
	function MeleeStat()
        while MeleeStat == true do
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint","Melee",1)
        end
	function DefenseStat()
        while DefenseStat == true do
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint","Defense",1)
        end
	function SwordStat()
        while SwordStat == true do
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint","Sword",1)
        end
	function GunStat()
        while MeleeStat == true do
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint","Melee",1)
        end
	function FruitStat()
        while FruitStat == true do
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint","Demon Fruit",1)
        end


-- Stats toggle Section

StatsTab:AddToggle({
	Name = "Melee Stat",
	Default = false,
	Callback = function(Value)
		_G.MeleeStat(Value)
        MeleeStat()
        end
})

StatsTab:AddToggle({
	Name = "Defense Stat",
	Default = false,
	Callback = function(Value)
		_G.DefenseStat(Value)
        DefenseStat()
        end
})

StatsTab:AddToggle({
	Name = "SwordStat",
	Default = false,
	Callback = function(Value)
		_G.SwordStat(Value)
        SwordStat()
        end
})

StatsTab:AddToggle({
	Name = "GunStat",
	Default = false,
	Callback = function(Value)
		_G.GunStat(Value)
        GunStat()
        end
})

StatsTab:AddToggle({
	Name = "Devil Fruit",
	Default = false,
	Callback = function(Value)
		_G.FruitStat(Value)
        FruitStat()
        end
})

--Toggles Section



